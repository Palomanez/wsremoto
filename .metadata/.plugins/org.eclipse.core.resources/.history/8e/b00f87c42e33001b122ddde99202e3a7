package variaveis;

public class TiposDados {

	public static void main(String[] args) {
		String nome = "Juliano Pedrosa";
		int idade = 32;
		double altura = 1.82;
		double peso = 108.0;
		System.out.println(nome);
		System.out.println(altura);
		System.out.println(peso);
		System.out.println(idade);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
